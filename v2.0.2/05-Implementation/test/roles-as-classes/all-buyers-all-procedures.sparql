# Story: list the 'default' buyers of all the procedures
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX epo: <http://data.europa.eu/a4g/ontology/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT ?procedure ?buyer
WHERE {
    ?procedureObject rdf:type epo:Procedure ;
               epo:title ?procedure ;
               epo:isResponsibilityOf ?BuyerObject .
    ?BuyerObject skos:prefLabel ?buyer .
}

