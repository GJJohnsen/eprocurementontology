# baseURI: http://example.org/epo/eucv/roles-taxonomy
# imports: http://www.w3.org/2004/02/skos/core#

@prefix : <http://example.org/epo/eucv/roles-taxonomy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/epo/eucv/roles-taxonomy>
  a owl:Ontology ;
  owl:imports skos: ;
.

:rts
  a skos:ConceptScheme ;
  skos:prefLabel "Role Taxonomy Scheme" ;
.

:role a skos:Concept ;
  rdfs:label "Role" ;
  skos:inScheme :rts ;
  skos:narrower :buy, :pm, :eo, :mdtr, :rvwr ;
.

:buy a skos:Concept ;
  skos:prefLabel "Buyer" ;
  skos:definition ""@en ;
  skos:narrowerTransitive :ce, :osca, :dc, :io ;
.

:ce a skos:Concept ;
  skos:prefLabel "Contracting Entity" ;
  skos:definition ""@en ;
  skos:narrowerTransitive :ceca, :pu, :eser ;
.

:osca a skos:Concept ;
  skos:prefLabel "Subsidised Organisation" ;
  skos:definition "Organisations awarding a contract subsidised by a Contracting Authority"@en ;
.

:dc a skos:Concept ;
  skos:prefLabel "Defence Contractor" ;
  skos:definition ""@en ;
.

:io a skos:Concept ;
  skos:prefLabel "International Organisation" ;
  skos:definition ""@en ;
.

:pm a skos:Concept ;
  skos:prefLabel "Procurement Manager" ;
  skos:definition ""@en ;
  skos:narrowerTransitive :psp, :cpbpm ;
.

:psp a skos:Concept ;
  skos:prefLabel "Procurement Service Provider" ;
  skos:definition ""@en ;
.

:eo a skos:Concept ;
  skos:prefLabel "Economic Operator" ;
  skos:definition ""@en ;
.

:rvwr a skos:Concept ;
  skos:prefLabel "Reviewer" ;
  skos:definition ""@en ;
.

:mdtr a skos:Concept ;
  skos:prefLabel "Mediator" ;
  skos:definition ""@en ;
.

:pu a skos:Concept ;
  skos:prefLabel "Public Undertaking" ;
  skos:definition ""@en ;
.

:eser a skos:Concept ;
  skos:prefLabel "Entity With Special or Exclusive Rigths" ;
  skos:definition ""@en ;
.

:subo a skos:Concept ;
  skos:prefLabel "Subcontractor" ;
  skos:definition ""@en ;
.

:eser a skos:Concept ;
  skos:prefLabel "Entity With Special-Exclusive Rigths" ;
  skos:definition ""@en ;
.

:tndr a skos:Concept ;
  skos:prefLabel "Tenderer" ;
  skos:definition ""@en ;
.

:win a skos:Concept ;
  skos:prefLabel "Winner" ;
  skos:definition ""@en ;
.

:sllr a skos:Concept ;
  skos:prefLabel "Seller" ;
  skos:definition ""@en ;
.

:dlvr a skos:Concept ;
  skos:prefLabel "Deliverer" ;
  skos:definition ""@en ;
.

:cga a skos:Concept ;
  skos:prefLabel "Central Government Authority" ;
  skos:definition "A central government contracting authority."@en ;
.

:rga a skos:Concept ;
  skos:prefLabel "Regional Government Authority" ;
  skos:definition "A regional government contracting authority."@en ;
.

:lga a skos:Concept ;
  skos:prefLabel "Local Government Authority" ;
  skos:definition "A local government contracting authority."@en ;
.

:cpbpm a skos:Concept ;
  skos:prefLabel "Central Purchasing Body Manager" ;
  skos:definition "A central purchasing body managing the procurement on behalf of one or more buyers."@en ;
.

:cpbca a skos:Concept ;
  skos:prefLabel "Central Purchasing Body Authority" ;
  skos:definition "A central purchasing body awarding the procurement."@en ;
.

:bgpl a skos:Concept ;
  skos:prefLabel "Body Governed by Public Law" ;
  skos:definition ""@en ;
.

:la a skos:Concept ;
  skos:prefLabel "Local Authority" ;
  skos:definition ""@en ;
.

:ra a skos:Concept ;
  skos:prefLabel "Regional Authority" ;
  skos:definition ""@en ;
.

:ca a skos:Concept ;
  skos:prefLabel "Contracting Authority" ;
  skos:definition ""@en ;
  skos:narrowerTransitive :cga, :ra, :la, :bgpl, :cpbca ;
.

:buyca: a skos:Concept ;
  skos:prefLabel "Buying Contracting Authority" ;
  skos:definition "Subtypes of contracting authorities acting as buyers."@en ;
  skos:exactMatch :ca ;
.

:ceca a skos:Concept;
  skos:prefLabel "Contracting Entity Authority" ;
  skos:definition "Subtypes of contracting authority acting as contracting entities, as defined in Directive 2014/25/EU."@en ;
  skos:exactMatch :ca ;
.

