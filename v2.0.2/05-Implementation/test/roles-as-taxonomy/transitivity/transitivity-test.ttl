@prefix : <http://data.europa.eu/a4g/ontology/cv/transitivity-test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europa.eu/a4g/ontology/cv/transitivity-test>
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.

:rts a skos:ConceptScheme ;
  skos:prefLabel "Role Taxonomy Scheme" ;
.

:role a skos:Concept ;
  rdfs:label "Role" ;
  skos:inScheme :rts ;
  skos:narrower :eo ;
.

:eo a skos:Concept ;
  skos:prefLabel "Economic Operator" ;
  skos:definition ""@en ;
  skos:narrower :tenderer ;
.

:tenderer a skos:Concept ;
  skos:prefLabel "Tenderer" ;
  skos:definition ""@en ;
  skos:narrower :winner ;
.

:winner a skos:Concept ;
  skos:prefLabel "Winner" ;
  skos:narrower :seller, :deliverer ;
.

:seller a skos:Concept ;
  skos:prefLabel "Seller" ;
.

:deliverer a skos:Concept ;
  skos:prefLabel "Deliverer" ;
  skos:definition ""@en ;
.

:buyer a skos:Concept ;
	skos:prefLabel "Buyer" ;
.
