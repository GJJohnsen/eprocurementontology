

@prefix : <http://data.europa.eu/a4g/ontology/cv/legal-form#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europa.eu/a4g/ontology/cv/legal-form>
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.

:lfs
  a skos:ConceptScheme ;
  skos:prefLabel "Legal Form Taxonomy" ;
.

:slu a skos:Concept ;
  rdfs:label "SLU" ;
  skos:definition "Sociedad Limitada Única"@es ;
  skos:inScheme :lfs 
.

:euinst a skos:Concept ;
  skos:prefLabel "European Institution" ;
  skos:definition "European Institution, e.g. Directorate General, European Agency"@en ;
.
:cgov a skos:Concept ;
  skos:prefLabel "Central Government" ;
  skos:definition "Central Government Authority."@en ;
.
:rgov a skos:Concept ;
  skos:prefLabel "Regional Government" ;
  skos:definition "Regional Government Authority."@en ;
.
:lgov a skos:Concept ;
  skos:prefLabel "Local Government" ;
  skos:definition "Local Government Authority."@en ;
.
:ltd a skos:Concept ;
  skos:prefLabel "Limited Corporation" ;
  skos:definition "A limited responsibility corporation."@en ;
.
:sa a skos:Concept ;
  skos:prefLabel "Società Anonima" ;
  skos:definition "A limited responsibility corporation in Italy"@en ;
.