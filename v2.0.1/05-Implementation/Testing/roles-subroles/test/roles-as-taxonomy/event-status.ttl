

@prefix : <http://data.europa.eu/a4g/ontology/cv/procurement-event-status#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europa.eu/a4g/ontology/cv/procurement-event-status>
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.

:pes
  a skos:ConceptScheme ;
  skos:prefLabel "Procurement Event Status" ;
.

:planned a skos:Concept ;
  rdfs:label "Planned" ;
  skos:definition "An event that has been planned to occur in the future."@es ;
  skos:inScheme :lfs 
.

:delayed a skos:Concept ;
  skos:prefLabel "Delayed" ;
  skos:definition "An event that is not expected to occur in the planned period because it has been postponed."@en ;
.

:cancelled a skos:Concept ;
  skos:prefLabel "Cancelled" ;
  skos:definition "An planned event that is has been removed."@en ;
.

:terminated a skos:Concept ;
  skos:prefLabel "Terminated" ;
  skos:definition "An event that was occurring but has been aborted prematurely."@en ;
.

:occurred a skos:Concept ;
  skos:prefLabel "Occurred" ;
  skos:definition "An event that has occurred in the past."@en ;
.
