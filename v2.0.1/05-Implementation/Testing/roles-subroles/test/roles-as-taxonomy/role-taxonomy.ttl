@prefix : <http://data.europa.eu/a4g/ontology/cv/roles-taxonomy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europa.eu/a4g/ontology/cv/roles-taxonomy>
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.

:rts
  a skos:ConceptScheme ;
  skos:prefLabel "Role Taxonomy Scheme" ;
.
:role a skos:Concept ;
  rdfs:label "Role" ;
  skos:inScheme :rts ;
  skos:narrower :buyer, :pm, :eo, :mediator, :reviewer ;
.
:buyer a skos:Concept ;
  skos:prefLabel "Buyer" ;
  skos:definition ""@en ;
  skos:narrower :ce, :buy_ca, :buy_osca, :buy_dc, :buy_io, :buy_cpb ;
.
:ce a skos:Concept ;
  skos:prefLabel "Contracting Entity" ;
  skos:definition ""@en ;
  skos:narrower :ce_ca, :ce_pu, :ce_eser ;
.
:buy_ca a skos:Concept ;
  skos:prefLabel "Buying Contracting Authority" ;
  skos:definition "Subtypes of contracting authorities acting as buyers."@en ;
  skos:narrower :buy_ca_budpay, 
                :buy_ca_execpay, 
                :buy_ca_infoprov, 
                :buy_ca_limprov, 
                :buy_ca_offlacc, 
                :buy_ca_processor, 
                :buy_ca_recipient, 
                :buy_ca_reqproc, 
                :buy_ca_reqrecep,
                :buy_ca_signatory, 
                :buy_ca_orgntr,
                :buy_ca_dlvree, 
                :buy_ca_invcee, 
                :buy_ca_awdec ;
.
:buy_osca a skos:Concept ;
  skos:prefLabel "Subsidised Organisation" ;
  skos:definition "Organisations awarding a contract subsidised by a Contracting Authority"@en ;
.
:buy_dc a skos:Concept ;
  skos:prefLabel "Defence Contractor" ;
  skos:definition ""@en ;
.
:buy_io a skos:Concept ;
  skos:prefLabel "International Organisation" ;
  skos:definition ""@en ;
.
:buy_cpb a skos:Concept ;
  skos:prefLabel "Buying Central Purchasing Body" ;
  skos:definition "A Central Purchasing Body buying and awarding."@en ;
  skos:narrower :buy_cpb_budpay, 
                :buy_cpb_execpay, 
                :buy_cpb_infoprov, 
                :buy_cpb_limprov, 
                :buy_cpb_offlacc, 
                :buy_cpb_processor, 
                :buy_cpb_recipient, 
                :buy_cpb_reqproc, 
                :buy_cpb_reqrecep,
                :buy_cpb_signatory, 
                :buy_cpb_orgntr,
                :buy_cpb_dlvree, 
                :buy_cpb_invcee, 
                :buy_cpb_awdec ;
.
:ce_ca a skos:Concept;
  skos:prefLabel "Contracting Entity Authority" ;
  skos:definition "Subtypes of contracting authority acting as contracting entities, as defined in Directive 2014/25/EU."@en ;
  skos:narrower :ce_ca_budpay, 
                :ce_ca_execpay, 
                :ce_ca_infoprov, 
                :ce_ca_limprov, 
                :ce_ca_offlacc, 
                :ce_ca_processor, 
                :ce_ca_recipient, 
                :ce_ca_reqproc, 
                :ce_ca_reqrecep,
                :ce_ca_signatory, 
                :ce_ca_orgntr,
                :ce_ca_dlvree, 
                :ce_ca_invcee, 
                :ce_ca_awdec ;
.
:ce_pu a skos:Concept ;
  skos:prefLabel "Public Undertaking" ;
  skos:definition ""@en ;
.
:ce_eser a skos:Concept ;
  skos:prefLabel "Entity With Special or Exclusive Rigths" ;
  skos:definition ""@en ;
.
:pm a skos:Concept ;
  skos:prefLabel "Procurement Manager" ;
  skos:definition ""@en ;
  skos:narrower :pm_psp, :pm_cpb ;
.
:pm_psp a skos:Concept ;
  skos:prefLabel "Procurement Service Provider" ;
  skos:definition ""@en ;
.
:pm_cpb a skos:Concept ;
  skos:prefLabel "Central Purchasing Body Manager" ;
  skos:definition "A central purchasing body managing the procurement on behalf of one or more buyers."@en ;
.
:eo a skos:Concept ;
  skos:prefLabel "Economic Operator" ;
  skos:definition ""@en ;
  skos:narrower :tenderer, :eo_grouplead, :reliedparty, :nonreliedparty, :solecontractor ;
.
:tenderer a skos:Concept ;
  skos:prefLabel "Tenderer" ;
  skos:definition ""@en ;
  skos:narrower :winner ;
.
:reliedparty a skos:Concept ;
  skos:prefLabel "Relied Party" ;
  skos:definition ""@en ;
.
:rp_subco a skos:Concept ;
  skos:prefLabel "Relied Party Subcontractor" ;
  skos:definition ""@en ;
.
:nonreliedparty a skos:Concept ;
  skos:prefLabel "Non-relied Party" ;
  skos:definition ""@en ;
.
:nrp_subco a skos:Concept ;
  skos:prefLabel "Non-relied Party Subcontractor" ;
  skos:definition ""@en ;
.
:solecontractor a skos:Concept ;
  skos:prefLabel "Sole Contractor" ;
  skos:definition ""@en ;
.
:winner a skos:Concept ;
  skos:prefLabel "Winner" ;
  skos:definition ""@en ;
  skos:narrower :seller, :deliverer ;
.
:seller a skos:Concept ;
  skos:prefLabel "Seller" ;
  skos:definition ""@en ;
.
:deliverer a skos:Concept ;
  skos:prefLabel "Deliverer" ;
  skos:definition ""@en ;
.
:reviewer a skos:Concept ;
  skos:prefLabel "Reviewer" ;
  skos:definition ""@en ;
.
:mediator a skos:Concept ;
  skos:prefLabel "Mediator" ;
  skos:definition ""@en ;
.
:eser a skos:Concept ;
  skos:prefLabel "Entity With Special-Exclusive Rigths" ;
  skos:definition ""@en ;
.

