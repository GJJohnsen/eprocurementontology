:ContactPoint1
  a :ContactPoint ;
  rdfs:label "Natalie Muric" ;
.
:Involvement_1
  a :ProcedureInvolvement ;
  :agent :OP ;
  :contactPoint :ContactPoint1 ;
  :period :period1 ;
