@prefix : <http://data.europa.eu/a4g/ontology/cv/roles-taxonomy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europa.eu/a4g/ontology/cv/roles-taxonomy>
  a owl:Ontology ;
  owl:imports skos: ;
.

:role_taxonomy
  a skos:ConceptScheme ;
  rdfs:label "role taxonomy" ;
.

:Role
  a skos:Concept ;
  rdfs:label "Role" ;
  skos:inScheme :role_taxonomy ;
  skos:narrower :Buyer ;
  skos:narrower :ProcurementManager ;
  skos:narrower :EconomicOperator ;
  skos:narrower :Mediator ;
  skos:narrower :Reviewer ;
.

:Buyer
  a skos:Concept ;
  rdfs:label "Buyer" ;
  skos:narrowerTransitive :ContractingEntity ;
  skos:narrower :exec_pay ;
  skos:narrower :group_lead ;
  skos:narrower :info_prov ;
  skos:narrower :processor ;
  skos:narrower :recipient ;
.

:CentralPurchasingBody
  a skos:Concept ;
  rdfs:label "central purchasing body" ;
  skos:narrower :bud_pay ;
  skos:narrower :exec_pay ;
  skos:narrower :info_prov ;
  skos:narrower :processor ;
  skos:narrower :recipient ;
.
:EconomicOperator
  a skos:Concept ;
  rdfs:label "Economic Operator" ;
  skos:narrower :group_lead ;
.
:Mediator
  a skos:Concept ;
  rdfs:label "information provider" ;
.
:ProcurementServiceProvider
  a skos:Concept ;
  rdfs:label "procurement service provider" ;
  skos:narrower :info_prov ;
  skos:narrower :processor ;
  skos:narrower :recipient ;
.
:Reviewer
  a skos:Concept ;
  rdfs:label "Reviewer" ;
.

:bud_pay
  a skos:Concept ;
  rdfs:label "manages budget" ;
  skos:broader :Buyer ;
  skos:broader :CentralPurchasingBody ;
.
:exec_pay
  a skos:Concept ;
  rdfs:label "executes payment" ;
  skos:broader :Buyer ;
.
:group_lead
  a skos:Concept ;
  rdfs:label "group lead" ;
  skos:broader :Buyer ;
  skos:broader :CentralPurchasingBody ;
  skos:broader :EconomicOperator ;
.
:info_prov
  a skos:Concept ;
  rdfs:label "information provider" ;
  skos:broader :Buyer ;
  skos:broader :CentralPurchasingBody ;
  skos:broader :ProcurementServiceProvider ;
.
:processor
  a skos:Concept ;
  rdfs:label "processor" ;
  skos:broader :Buyer ;
  skos:broader :CentralPurchasingBody ;
  skos:broader :ProcurementServiceProvider ;
.
:recipient
  a skos:Concept ;
  rdfs:label "recipient" ;
  skos:broader :Buyer ;
  skos:broader :CentralPurchasingBody ;
  skos:broader :ProcurementServiceProvider ;
.

